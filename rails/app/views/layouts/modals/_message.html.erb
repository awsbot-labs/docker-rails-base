                            <a class="font-s13" href="javascript:void(0)"><%= message.email %></a>
                        </div>
                        <div class="block-content">
                            <%= message.body %>
                        </div>
                        <div class="block-content bg-gray-lighter">
                            <div class="row items-push">
                                <div class="col-sm-4">
                                    <div class="img-container fx-img-zoom-in">
